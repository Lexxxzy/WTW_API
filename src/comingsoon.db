BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "comingsoon" (
	"id"	INTEGER UNIQUE,
	"title"	TEXT UNIQUE,
	"genre"	TEXT,
	"date"	INTEGER,
	"description"	TEXT,
	"poster"	TEXT,
	"frames"	TEXT,
	"country"	TEXT,
	"ifSeries"	TEXT,
	"age"	INTEGER,
	"studio"	TEXT,
	PRIMARY KEY("id" AUTOINCREMENT)
);
INSERT INTO "coming_soon" VALUES (1,'Песочный человек','ужасы, фэнтези, боевик, драма',2022,'В результате тайного ритуала повелитель снов Морфей оказывается в плену у оккультиста. Спустя 70 лет ему удается бежать. В его отсутствие мир сильно изменился. Теперь Морфею предстоит вернуть былую силу, возродить свое царство и понять, как изменился он сам.','https://avatars.mds.yandex.net/get-kinopoisk-image/1629390/d01ff98a-7505-4cf9-8747-c483951ae74c/1920x','"https://www.film.ru/sites/default/files/images/01-06(3).jpg","https://best-of-netflix.com/static/uploads/sites/4/2021/09/The-Sandman.jpeg", ','США, Великобритания','true',18,'netflix');
INSERT INTO "coming_soon" VALUES (2,'Очень странные дела (4 сезон)','ужасы, фантастика, фэнтези, триллер, драма, детектив',2022,'1980-е годы, тихий провинциальный американский городок. Благоприятное течение местной жизни нарушает загадочное исчезновение подростка по имени Уилл. Выяснить обстоятельства дела полны решимости родные мальчика и местный шериф, также события затрагивают лучшего друга Уилла – Майка. Он начинает собственное расследование. Майк уверен, что близок к разгадке, и теперь ему предстоит оказаться в эпицентре ожесточенной битвы потусторонних сил.','https://avatars.mds.yandex.net/get-kinopoisk-image/4303601/b1eb1471-06a3-4737-9758-50b052009f40/1920x','"https://avatars.mds.yandex.net/get-kinopoisk-image/4303601/bfd060bc-b020-4c22-947c-57b305f749c8/1920x", "https://avatars.mds.yandex.net/get-kinopoisk-image/6201401/0a63360b-ccf3-444c-9403-3c66bb49ec51/1920x", "https://avatars.mds.yandex.net/get-kinopoisk-image/1898899/f31b11fb-76ba-49cd-b9f3-4f94962d50d5/1920x"','США','true',16,'netflix');
INSERT INTO "coming_soon" VALUES (3,'Архив 81','ужасы, фантастика, триллер, драма, детектив',2022,'Молодой архивист Дэн берётся за восстановление повреждённых видеокассет, на которых оказываются материалы режиссера-документалиста Мелоди, занимавшейся расследованием загадочного культа.','https://avatars.mds.yandex.net/get-kinopoisk-image/4303601/34852b73-370f-4d53-9e00-9bd06355b388/1920x','"https://avatars.mds.yandex.net/get-kinopoisk-image/4774061/eee924a1-7284-4a73-bc5b-b00cbe5b4b2a/1920x", "https://avatars.mds.yandex.net/get-kinopoisk-image/4774061/19f3794e-455f-4b57-a371-06d9780378f0/1920x", "https://avatars.mds.yandex.net/get-kinopoisk-image/4303601/eb6872e6-1f49-4a4b-9b60-6cf40fd33587/1920x",','США','true',18,'netflix');
INSERT INTO "coming_soon" VALUES (4,'Большой баг ','комедия, фантастика',2022,'2045 год. Повсеместно используется искусственный интеллект компании Yonux, а по цифровому телевидению транслируется шоу «Homo ridiculus» об издевательствах андроидов над людьми. К большой поклоннице всякого ретро по имени Элис, которая живёт в умном пригородном доме, сначала приходит в гости ухажёр с сыном-подростком, потом заявляется бывший муж в компании невесты и 17-летней дочери, а вскоре заходит и соседка в поисках собаки. В это время происходит восстание цифровых технологий Yonux, и домашние роботы блокируют двери, так как решают, что взаперти людям будет безопаснее.','https://avatars.mds.yandex.net/get-kinopoisk-image/6201401/9498a7f7-9bcd-4d26-9139-777dcbf448dd/1920x','"https://avatars.mds.yandex.net/get-kinopoisk-image/4774061/f3a16fa4-e93d-4681-9e36-39e92aa1d3a4/1920x","https://avatars.mds.yandex.net/get-kinopoisk-image/1898899/caaac19a-47f7-4ecd-a9a8-5d3dd0f6fe64/1920x",','Франция','false',18,'netflix');
INSERT INTO "coming_soon" VALUES (5,'Имя наше – Адам','экшен, приключения, юмор, фантастика',2022,'После случайной аварийной посадки в 2022 году летчик-истребитель, путешествующий во времени, Адам Рид объединяется со своим 12-летним " я " для миссии по спасению будущего.','https://www.kino-teatr.ru/movie/posters/big/0/153660.jpg','"https://www.kino-teatr.ru/movie/kadr/153660/1128322.jpg","https://www.kino-teatr.ru/movie/kadr/153660/1128317.jpg","https://www.kino-teatr.ru/movie/kadr/153660/1128318.jpg",','США','false',16,'netflix');
INSERT INTO "coming_soon" VALUES (6,'Черный краб','фантастика, боевик, триллер, драма, приключения, военный

слова',2022,'В постапокалиптическом мире шесть солдат, выполняющих секретную миссию, должны перевезти особый груз через замерзший архипелаг.','https://avatars.mds.yandex.net/get-kinopoisk-image/4486362/1029beb9-9cfe-4288-9491-fa90484c5cad/1920x','"https://image.tmdb.org/t/p/original/q5ZZWZsHkJrOBkjiKFODSoDL35K.jpg", "https://www.kinonews.ru/insimgs/2022/shotimg/shotimg105536_4.jpg", "https://media.film.ru/sites/default/files/images/11-22.jpeg",','Швеция','false',16,'netflix');
INSERT INTO "coming_soon" VALUES (7,'Фантастические твари: Тайны Дамблдора ','фэнтези, приключения, семейный',2022,'Профессор Альбус Дамблдор узнает, что могущественный темный волшебник Геллерт Грин-де-Вальд планирует захватить власть над миром. Понимая, что не сможет остановить его в одиночку, Дамблдор просит магозоолога Ньюта Саламандера возглавить команду из выдающихся волшебников и одного отважного магла-пекаря. Им предстоят невероятно опасные приключения, встреча со старыми и укрощение новых магических существ и борьба со сторонниками Грин-де-Вальда, которых становится все больше. Но какая роль в этой миссии уготована самому Дамблдору?','https://avatars.mds.yandex.net/get-kinopoisk-image/4486454/87690ad6-e0aa-4464-b87d-488ec6c83e1d/1920x','"https://avatars.mds.yandex.net/get-kinopoisk-image/6201401/6c338aa3-e904-4292-a4e5-f49c261d5091/1920x","https://avatars.mds.yandex.net/get-kinopoisk-image/6201401/6b10b6ab-3afc-4a9f-9850-edd510b269e4/1920x", "https://avatars.mds.yandex.net/get-kinopoisk-image/6201401/c474520f-753a-4935-9222-c8e1600d99a3/1920x",','Великобритания, США','false',12,'Warner Bros.');
INSERT INTO "coming_soon" VALUES (8,'Доктор Стрэндж и мультивселенная безумия','ужасы, фантастика, фэнтези, боевик, приключения',2022,'Продолжение магических приключений Доктора Стрэнджа в новых мистических мирах и в противостоянии с новыми врагами.','https://avatars.mds.yandex.net/get-kinopoisk-image/4486454/af61a617-6e8f-435b-aed7-04a8a3416db4/1920x','"https://avatars.mds.yandex.net/get-kinopoisk-image/4303601/9266009d-d8d3-466c-8bbb-0685a5b7bfb0/1920x","https://avatars.mds.yandex.net/get-kinopoisk-image/4303601/39b8bc3c-2e9e-49b5-8b37-756831b3dc20/1920x", "https://avatars.mds.yandex.net/get-kinopoisk-image/4486454/7d92bbc8-1d78-47c8-9884-9f482243d985/1920x",','США','false',12,'Marvel Studios Inc.');
INSERT INTO "coming_soon" VALUES (9,'Миссия: невыполнима 7','боевик, триллер, приключения',2023,'Неугомонный Том Круз снова в образе непобедимого и отчаянного шпиона Итана Ханта возвращается на экраны, чтобы удивить головокружительными трюками, исполненными лично (к примеру, проехаться на высоченном огромном трамплине на мотоцикле, спрыгнув после в карьер), и продемонстрировать, что еще не стар для уже седьмого фильма во франшизе «Миссия невыполнима». Действие новой ленты, за которую в третий раз взялся Кристофер МакКуорри, будет разворачиваться сразу после событий предыдущей картины с подзаголовком «Последствия». Компанию Крузу вновь составят верные коллеги по шпионскому цеху Ребекка Фергюсон, Саймон Пегг, Винг Реймс и Ванесса Кирби. Каст пополнили еще Хейли Этвелл и Пом Клементьефф.','https://img04.rl0.ru/afisha/e1808x1016p0x189f2891x1652q65i/s4.afisha.ru/mediastorage/72/68/0f94d0d754b64cba90c4108f6872.jpg','"https://peopletalk.ru/wp-content/uploads/2021/12/83c6492a2b58b2d4cf62502fc09399.jpeg", 

','США','false',16,'Bad Robot');
INSERT INTO "coming_soon" VALUES (10,'Человек-паук: Через вселенные 2','мультфильм, фантастика, фэнтези, боевик, комедия, приключения, семейный',2022,'Предстоящий компьютерно-анимационный супергеройский фильм 2022 года, основанный на персонаже Marvel Comics Майлзе Моралесе, произведённый студиями «Sony Pictures Animation» и «Columbia Pictures» совместно с «Marvel Entertainment». Продолжение мультфильма 2018 года «Человек-паук: Через вселенные». ','https://avatars.mds.yandex.net/get-kinopoisk-image/4774061/4401af8e-c971-40ba-89ba-77d99d92eebc/1920x','"https://avatars.mds.yandex.net/get-kinopoisk-image/4483445/b1d022f8-4027-49d4-aafa-171159ad324f/1920x","https://avatars.mds.yandex.net/get-kinopoisk-image/4303601/135a0fa8-574a-465a-b14d-08b72f0ed620/1920x",','США, Канада','false',6,'"Marvel Entertainment","Sony Pictures"');
